if [ $# -eq 1 ]; then
  if [ -e $1 ]; then #-e es un directorio o con -d
    if [ -f $1 ]; then  #-f es un archivo
      echo " $1 es un archivo "
    else
      echo " $1 es un directorio "
    fi
  else
    echo " $1 no existe "
    mkdir $1
  fi
else
  echo " Cantidad de parametros incorrecta 
fi
